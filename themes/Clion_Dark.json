{
    "name": "CLion Dark",
    "$schema": "vscode://schemas/color-theme",
    "semanticHighlighting": true,
    "include": "./JetBrains New UI Dark-color-theme.json",
    "colors": {
        "statusBar.debuggingBackground": "#d47c00",
        "editor.findMatchBackground": "#114957",
        "editor.selectionBackground": "#214283",
        "editor.wordHighlightBackground": "#373b3960",
        "editor.findMatchHighlightBackground": "#11495750",
        // "editor.findMatchHighlightBorder":"#9DA0A8",
        "editor.findMatchBorder": "#9DA0A8",
        // "editor.findMatchBorder":"#11495780"
    },
    "semanticTokenColors": {
        "function.builtin:python": "#8888C6",
        "class:cpp":"#b9bcd1",
        "class.deduced.defaultLibrary.globalScope:cpp":"#CF8E6D",
        "class.declaration.definition.constructorOrDestructor.classScope:cpp":"#56a8f5",
        "class.declaration.declaration.constructorOrDestructor.classScope:cpp":"#56a8f5",
        "function:cpp":"#56a8f5",
        "function.globalScope:cpp":"#bcbec4",
        "function.declaration.globalScope:cpp":"#56a8f5",
        "method:cpp":"#56a8f5",
        "method.classScope:cpp":"#b9bcd1",
        "class.globalScope:cpp":"#b5b6e3",
        "method.declaration.definition.classScope:cpp":"#56a8f5",
        "class.declaration.definition.globalScope:cpp":"#56a8f5",
        "*.decorator:python": "#debc7b",
        "selfParameter:python": "#94558D",
        "parameter:python": "#B189F5",
        "magicFunction:python": "#B200B2",
        "function.declaration:python": "#debc7b",
        "method:python": "#debc7b"
    },
    "tokenColors": [
        {
            "scope": [
                "keyword.control",
                "storage.type"
            ],
            "settings": {
                "foreground": "#749DED",
            },
        },
        {
            "scope": "meta.function-call.arguments.python",
            "settings": {
                "foreground": "#CED0D6",
                "fontStyle": ""
            },
        },
        {
            "scope": "string.quoted.single.python",
            "settings": {
                "foreground": "#7EC482",
                "fontStyle": ""
            }
        },
        {
            "scope": "string.quoted.binary.single.python",
            "settings": {
                "foreground": "#A5C261",
                "fontStyle": ""
            }
        },
        {
            "scope": "meta.function-call.python",
            "settings": {
                "foreground": "#debc7b",
                "fontStyle": ""
            }
        },
        {
            "scope": "variable.parameter.function-call.python",
            "settings": {
                "foreground": "#AA4926",
                "fontStyle": ""
            }
        },
        {
            "scope": "constant.numeric.dec.python",
            "settings": {
                "foreground": "#D981DE",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "keyword.control.directive.include.cpp",
                "meta.preprocessor.include.cpp",
                "keyword.control.directive.endif.cpp",
                "keyword.control.directive.define.cpp",
                "keyword.control.directive.conditional.ifndef.cpp",
                "keyword.control.directive.conditional.ifdef.cpp",
                "source.cpp"
            ],
            "name": "c++ include",
            "settings": {
                "foreground": "#b3ae60",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "string.quoted.other.lt-gt.include.cpp",
                "meta.preprocessor.include.cpp",
                "source.cpp"
            ],
            "name": "c++ include file",
            "settings": {
                "foreground": "#6aab73",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "entity.name.namespace.cpp",
                "entity.name.scope-resolution.cpp",
                "meta.body.function.definition.cpp",
                "meta.function.definition.cpp",
                "source.cpp"
            ],
            "name": "c++ namespace",
            "settings": {
                "foreground": "#b5b6e3",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "entity.name.scope-resolution.function.call.cpp",
                "entity.name.scope-resolution.function.definition.cpp",
                "meta.body.function.definition.cpp",
                "meta.function.definition.cpp",
                "source.cpp",
                // "entity.name.type.class.cpp",
                "support.type.built-in.posix-reserved.cpp",
                "support.type.posix-reserved.cpp"
            ],
            "name": "c++ fmt::",
            "settings": {
                "foreground": "#b5b6e3",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "storage.type.namespace.directive.cpp",
                "storage.type.namespace.definition.cpp",
                "storage.type.class.cpp",
                "storage.type.modifier.access.control.public.cpp",
                "storage.type.modifier.access.control.private.cpp",
                "storage.type.modifier.access.control.protected.cpp",
                "storage.type.built-in.primitive.cpp",
                "storage.type.primitive.cpp",
                "meta.body.function.definition.cpp",
                "meta.function.definition.cpp",
                "storage.type.template.cpp",
                "storage.type.template.argument.typename.cpp",
                "source.cpp",
                "keyword.control.throw.cpp",
                "keyword.control.while.cpp",
                "keyword.control.return.cpp",
                "keyword.control.continue.cpp",
                "storage.type.modifier.access.public.cpp",
                "keyword.control.if.cpp",
                "keyword.control.for.cpp"
            ],
            "name": "c++ auto",
            "settings": {
                "foreground": "#cf8e6d",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "punctuation.separator.scope-resolution.namespace.using.cpp",
                "punctuation.separator.namespace.access.cpp",
                "meta.using-namespace.cpp",
                "punctuation.separator.colon.inheritance.cpp",
                "source.cpp"
            ],
            "name": "c++ ::",
            "settings": {
                "foreground": "#bcbec4",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "entity.name.function.call.cpp",
                "entity.name.function.cpp",
                "meta.function.definition.body.lambda.cpp",
                "meta.body.function.definition.cpp",
                "meta.function.definition.cpp",
                "source.cpp"
            ],
            "name": "c++ ::println",
            "settings": {
                "foreground": "#bcbec4",
                "fontStyle": ""
            },
        },
        {
            "scope": [
                "support.function.cpp",
                "punctuation.terminator.statement.cpp",
                "storage.modifier.reference.cpp",
                "meta.function.definition.parameters.lambda.cpp",
                "entity.name.type.parameter.cpp",
                "meta.parameter.cpp",
                "meta.assignment.cpp",
                "entity.name.function.member.cpp",
                "meta.body.function.definition.cpp",
                "meta.function.definition.cpp",
                "punctuation.separator.dot-access.cpp",
                "keyword.other.unit.user-defined.cpp",
                "keyword.other.suffix.literal.user-defined.integer.cpp",
                "storage.modifier.pointer.cpp",
                "punctuation.separator.delimiter.comma.cpp",
                "source.cpp"
            ],
            "name": "c++ ",
            "settings": {
                "foreground": "#bcbec4",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "entity.name.type.cpp",
                "entity.name.type.parameter.cpp",
                "storage.type.built-in.cpp",
                "entity.name.type.template.cpp",
                "storage.type.cpp"
            ],
            "name": "c++ std::string string",
            "settings": {
                "foreground": "#b9bcd1",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "punctuation.section.block.end.bracket.curly.cpp",
                "punctuation.section.block.begin.bracket.curly.cpp",
                "punctuation.definition.parameters.begin.lambda.cpp",
                "punctuation.definition.parameters.end.lambda.cpp",
            ],
            "name": "c++ {std::string string}",
            "settings": {
                "foreground": "#54a857",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "constant.numeric.decimal.cpp",
                "keyword.other.unit.suffix.integer.cpp",
                "keyword.other.suffix.literal.built-in.integer.cpp",
                "meta.function.definition.cpp",
                "meta.body.function.definition.cpp",
                "source.cpp"
            ],
            "name": "c++ 1234",
            "settings": {
                "foreground": "#2aacb8",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "punctuation.section.arguments.begin.bracket.round.function.call.cpp",
                "punctuation.section.arguments.begin.bracket.round.function.member.cpp",
                "punctuation.section.arguments.end.bracket.round.function.call.cpp",
                "punctuation.section.arguments.end.bracket.round.function.member.cpp",
                "punctuation.section.parameters.begin.bracket.round.cpp",
                "punctuation.section.parameters.end.bracket.round.cpp",
                "punctuation.definition.capture.begin.lambda.cpp",
                "punctuation.definition.capture.end.lambda.cpp",
                "punctuation.section.block.begin.bracket.curly.function.definition.cpp",
                "punctuation.section.block.begin.bracket.curly.class.cpp",
                "punctuation.section.block.end.bracket.curly.function.definition.cpp",
                "punctuation.section.block.end.bracket.curly.class.cpp",
                "punctuation.section.angle-brackets.begin.template.call.cpp",
                "punctuation.section.angle-brackets.end.template.call.cpp",
                "keyword.operator.comparison.cpp",
                "punctuation.section.parameters.begin.bracket.round.special.constructor.cpp",
                "punctuation.section.parameters.end.bracket.round.special.constructor.cpp",
                "punctuation.section.angle-brackets.begin.template.definition.cpp",
                "punctuation.section.angle-brackets.end.template.definition.cpp"
            ],
            "name": "c++ 括号",
            "settings": {
                "foreground": "#e8ba36",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "entity.name.function.preprocessor.cpp",
            ],
            "name": "c++ 预处理",
            "settings": {
                "foreground": "#908b25",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "variable.other.property.cpp",
            ],
            "name": "c++ 成员变量",
            "settings": {
                "foreground": "#9373a5",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "entity.name.function.definition.special.constructor.cpp",
                "entity.name.function.constructor.cpp"
            ],
            "name": "c++ c成员函数",
            "settings": {
                "foreground": "#56a8f5",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "entity.name.function.definition.special.constructor.cpp",
                "entity.name.scope-resolution.function.call.cpp",
                "entity.name.function.definition.special.member.destructor.cpp",
                "entity.name.function.destructor.cpp",
                "entity.name.function.constructor.cpp"
